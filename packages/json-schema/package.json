{
  "name": "@stackmeister/json-schema",
  "version": "0.1.5",
  "license": "MIT",
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "scripts": {
    "build": "tsc && babel src --out-dir cjs --extensions .ts,.tsx --delete-dir-on-start && tsc --emitDeclarationOnly --declarationDir cjs",
    "test": "jest --passWithNoTests --collectCoverage",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "docs:build": "typedoc --out docs/json-schema src/index.ts",
    "docs:serve": "typedoc --out docs/json-schema src/index.ts && serve docs/json-schema",
    "release": "yarn publish",
    "upgrade:latest": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@stackmeister/equals": "^0.1.6",
    "@stackmeister/json-pointer": "^0.1.6",
    "@stackmeister/types": "^0.1.7",
    "@stackmeister/uri": "^0.1.8",
    "@stackmeister/json-ref": "^0.1.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "serve": "^14.2.1",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "url": "git://github.com/StackmeisterGmbH/typescript-packages",
    "type": "git",
    "directory": "packages/json-schema"
  },
  "publishConfig": {
    "access": "public"
  }
}
