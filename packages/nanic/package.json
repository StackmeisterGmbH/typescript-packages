{
  "name": "@stackmeister/nanic",
  "version": "0.1.0",
  "license": "MIT",
  "main": "esm/cli/main.js",
  "type": "module",
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "tsc && babel src --out-dir cjs --extensions .ts,.tsx --delete-dir-on-start && tsc --emitDeclarationOnly --declarationDir cjs",
    "test": "jest --passWithNoTests --collectCoverage",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "release": "yarn publish",
    "upgrade:latest": "npx npm-check-updates -u"
  },
  "bin": {
    "nanic": "esm/cli/main.js"
  },
  "dependencies": {
    "@stackmeister/types": "^0.1.7",
    "cli-highlight": "^2.1.11",
    "commander": "^9.4.1",
    "debug": "^4.3.4",
    "expression-eval": "^5.0.0",
    "glob": "^8.0.3",
    "install": "^0.13.0",
    "npm": "^9.2.0",
    "path-to-regexp": "^6.2.1",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "serve": "^14.1.1",
